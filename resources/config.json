{  
  "proxyUrl": "https://github.com",  
  "allowSelfSignedCert": true,
  "tempPassword": {
    "sendEmail": false,
    "emailConfig": {
    }
  },
  "authorisedList": [{    
  }],
  "sink": [
    {
      "type": "fs",
      "params": {
        "filepath": "./."
      },
      "enabled": false
    },
    {
      "type": "mongo",
      "connectionString": "mongodb://localhost:37017",
      "options": {
        "useNewUrlParser": true,
        "useUnifiedTopology": true,
        "sslValidate": false,
        "ssl": true
      },
      "enabled": true
    }
  ],
  "authentication": [{
    "type": "local",
    "enabled": true
  }],
  "smtpHost": "...",
  "smtpPort": 25
}
