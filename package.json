{
  "name": "@finos/git-proxy",
  "version": "1.3.2",
  "description": "Deploy custom push protections and policies on top of Git.",
  "scripts": {
    "cli": "node ./packages/git-proxy-cli/index.js",
    "client": "vite --config vite.config.js",
    "clientinstall": "npm install --prefix client",
    "server": "node dist/index.js",
    "start": "concurrently 'npm run server' 'npm run client'",
    "build": "tsc && node ./scripts/copy-files.js",
    "dev": "ts-node src/index.ts",
    "test": "NODE_ENV=test cross-env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
    "test-coverage": "nyc npm run test",
    "test-coverage-ci": "nyc --reporter=lcovonly --reporter=text npm run test",
    "prepare": "node ./scripts/prepare.js",
    "lint": "eslint index.ts \"src/**/*.{js,jsx,ts,tsx,json}\" \"test/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:fix": "eslint --fix \"index.ts src/**/*.{js,jsx,ts,tsx,json}\" \"test/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json,scss} test/**/*.{js,jsx,ts,tsx,json} --config ./.prettierrc",
    "gen-schema-doc": "node ./scripts/doc-schema.js",
    "cypress:run": "cypress run"
  },
    "cypress:run": "cypress run"
  },
  "bin": {
    "git-proxy": "./index.js",
    "git-proxy": "./index.js",
    "git-proxy-all": "concurrently 'npm run server' 'npm run client'"
  },
  "workspaces": [
    "./packages/git-proxy-cli"
    "./packages/git-proxy-cli"
  ],
  "author": "Paul Groves",
  "license": "Apache-2.0",
  "author": "Paul Groves",
  "license": "Apache-2.0",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "4.11.3",
    "@primer/octicons-react": "^19.8.0",
    "@seald-io/nedb": "^4.0.2",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "bit-mask": "^1.0.2",
    "body-parser": "^1.20.1",
    "chai-http": "^4.3.0",
    "classnames": "2.5.1",
    "concurrently": "^8.0.0",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "cypress": "^13.11.0",
    "diff2html": "^3.4.33",
    "express": "^4.18.2",
    "express-http-proxy": "^2.0.0",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.1",
    "history": "5.3.0",
    "jsonschema": "^1.4.1",
    "load-plugin": "^6.0.0",
    "lodash": "^4.17.21",
    "lusca": "^1.7.0",
    "moment": "^2.29.4",
    "mongodb": "^5.0.0",
    "nodemailer": "^6.6.1",
    "passport": "^0.7.0",
    "passport-activedirectory": "^1.0.4",
    "passport-local": "^1.0.0",
    "perfect-scrollbar": "^1.5.5",
    "prop-types": "15.8.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-html-parser": "^2.0.2",
    "react-router-dom": "6.25.1",
    "uuid": "^10.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.8.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.12",
    "@types/yargs": "^17.0.32",
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.8.0",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.8.0",
    "husky": "^9.0.0",
    "mocha": "^10.2.0",
    "nyc": "^17.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0",
    "vite": "^4.4.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@4.3.1"
}
